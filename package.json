{
  "name": "chess-positions-queue-processor",
  "version": "0.3.0",
  "description": "Process queue of chess positions which needs to be analyzed",
  "main": "processor.js",
  "scripts": {
    "test": "jasmine",
    "tdd": "nodemon --exec jasmine",
    "coverage": "nyc --reporter=lcov npm test",
    "tdd-coverage": "nodemon --exec npm run coverage",
    "report-coverage": "nyc npm test && nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "e2e": "jasmine ./spec/e2e.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Scorpibear/chess-positions-queue-processor.git"
  },
  "keywords": [
    "chess",
    "fen",
    "analysis",
    "queue",
    "processor",
    "positions"
  ],
  "author": "Mikhail Subach",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Scorpibear/chess-positions-queue-processor/issues"
  },
  "homepage": "https://github.com/Scorpibear/chess-positions-queue-processor#readme",
  "devDependencies": {
    "eslint": "^5.5.0",
    "jasmine": "^3.2.0",
    "nodemon": "^1.18.4",
    "nyc": "^13.0.1"
  },
  "nyc": {
    "exclude": [
      "spec/**/*.spec.js"
    ]
  }
}
